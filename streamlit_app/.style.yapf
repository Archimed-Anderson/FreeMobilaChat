[style]
based_on_style = pep8
column_limit = 100
indent_width = 4
use_tabs = False
blank_line_before_nested_class_or_def = False
allow_multiline_dictionary_keys = True
allow_multiline_lambdas = True
allow_multiline_parameters = True
allow_split_before_default_or_named_assigns = True
allow_split_before_dict_value = True
allow_split_before_expression_after_opening_paren = True
allow_split_before_first_argument = True
allow_split_before_logical_operator = True
allow_split_before_ternary_operator = True
coalesce_brackets = True
dedent_closing_brackets = True
each_dict_entry_on_separate_line = False
indent_closing_brackets = False
join_multiple_lines = False
space_between_ending_comma_and_closing_bracket = False
split_arguments_when_comma_terminated = True
split_before_arithmetic_operator = False
split_before_bitwise_operator = False
split_before_closing_bracket = True
split_before_comment = True
split_before_dict_set_generator = True
split_before_dot = False
split_penalty_arithmetic_operator = 300
split_penalty_after_getattr = 30
split_penalty_before_bitwise_operator = 300
split_penalty_before_logical_operator = 300
split_penalty_comment = 300
split_penalty_extra_indent = 30
split_penalty_for_added_line_split = 30
split_penalty_import_names = 300
split_penalty_imports = 300
split_penalty_logical_operator = 300